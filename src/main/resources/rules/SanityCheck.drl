package com.resources;

import com.model.RoleInfo;

// SanityCheck rules go here:

rule "SanityCheck"
	dialect "mvel"
	no-loop true
	ruleflow-group "sanitycheck"
	when
		r: RoleInfo( roleTargetCapacity < 0)
	then
		modify(r) {
			setRoleTargetCapacity(0)
		}
		System.out.println("Fired Sanity Check");
end
